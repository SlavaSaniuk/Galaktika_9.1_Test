.create view FROMKATMC
var pNrec:comp;
from
  SpSopr,
  KatMc,
  KatUsl,
  KatParty,
  KatEd,
  KatOtpEd,
  KatOtpEd KatEdGr

where
((
  pNrec         == SpSopr.NRec   and
  SpSopr.cMCUSL == KatMC.NRec    and
  SpSopr.cMCUSL == KatUsl.NRec   and
  SpSopr.cParty == KatParty.NRec and
  SpSopr.cOtpEd == KatOtpEd.NRec
))
;
