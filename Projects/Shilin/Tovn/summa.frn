.create view FORKATSOPR
var pNrec:comp;
from
  katsopr,
  Dover,
  KatOrg
where
((
  pNRec          == KatSopr.NRec  and
  KatSopr.cDover == Dover.NRec    and
  Dover.cPost    == KatOrg.NRec
